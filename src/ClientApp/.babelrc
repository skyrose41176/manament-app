{
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".ts", ".json"],
        "root": ["./src"],
        "alias": {
          "@api": "./src/apis",
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@configs": "./src/configs",
          "@constants": "./src/constants",
          "@hooks": "./src/hooks",
          "@redux": "./src/redux",
          "@screens": "./src/screens",
          "@navigation": "./src/navigation",
          "@services": "./src/services",
          "@styles": "./src/styles",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
